{
  "version": 2,
  "name": "stone-shop-production",
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "ENVIRONMENT": "production"
  },
  "github": {
    "enabled": true
  }
}